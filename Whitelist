local playersToKeep = {
    "Xariaqq",
    "zkxkira",
    "camisnotcoollbozo",
    "ROBLOXS_BROTHERRRRRR",
    "jedelmx89alt",
    "DomatesHayat",
    "Adriane_Gamer123",
}

local function isPlayerInList(player)
    local playerName = player.Name:lower()
    for _, name in pairs(playersToKeep) do
        if playerName == name:lower() then
            return true
        end
    end
    return false
end

local function kickUnwantedPlayers(player)
    if not isPlayerInList(player) then
        player:Kick("Not Whitelisted? Imagine.")
    end
end

game.Players.PlayerAdded:Connect(kickUnwantedPlayers)
