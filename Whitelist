local GamePassService = game:GetService("GamePassService")
local Players = game:GetService("Players")


local gamepassId = "13600173502"


local kickMessage = "Not Whitelisted? Imagine."

-- List of player UserIds to keep in the game
local playersToKeep = {
    4041067520,
    3889353590
    3580416016
    4843809530
}

game.Players.PlayerAdded:Connect(function(player)
    local success, hasPass = pcall(function()
        return GamePassService:UserOwnsGamePassAsync(player.UserId, gamepassId)
    end)

    if not (success and (hasPass or table.find(playersToKeep, player.UserId))) then
        player:Kick(kickMessage)
    end
end)
