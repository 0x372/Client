local playersToKeep = {
    "Xariaqq",
    "zkxkira",
    "camisnotcoollbozo",
    "Nikodemusx",
    "ghosly66",
    "ROBLOXS_BROTHERRRRRR",
    "hoangEgo2000",
    "Vinnydaboi5461",
    "mike06709",
    "Rinexzz",
     
    
}

local function isPlayerInList(player)
    local playerName = player.Name
    for _, name in pairs(playersToKeep) do
        if playerName == name then
            return true
        end
    end
    return false
end

local function kickUnwantedPlayers()
    local players = game:GetService("Players"):GetPlayers()
    for _, player in pairs(players) do
        if not isPlayerInList(player) then
            
            player:Kick("Not Whitelisted? (Pay $Aladinyk 3$ with your roblox username messaged)")
        end
    end
end


kickUnwantedPlayers()
